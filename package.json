{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dbmovie",
  "title": "DBMovie",
  "description": "",
  "icon": "command-icon.png",
  "author": "Leixingxing",
  "owner": "austinlei",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Show List and Detail",
      "description": "Template for showing a static list and a detail view",
      "mode": "view",
      "arguments": [
        {
          "name": "title",
          "placeholder": "Title",
          "type": "text",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.2",
    "dom-parser": "^0.1.6",
    "htmlparser2": "^8.0.1",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/dom-parser": "^0.1.1",
    "@types/jsdom": "^20.0.1",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
